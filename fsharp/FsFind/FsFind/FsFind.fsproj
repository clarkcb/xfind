<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <PackageId>FsFind</PackageId>
    <PackageVersion>0.1.0</PackageVersion>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>Cary Clark</Authors>
    <Product>FsFind</Product>
    <Description>F# implementation of xfind in dotnet 3.1</Description>
    <RootNamespace>FsFind</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="Common.fs" />
    <Compile Include="Color.fs" />
    <Compile Include="EmbeddedResource.fs" />
    <Compile Include="FindException.fs" />
    <Compile Include="FileUtil.fs" />
    <Compile Include="FileTypes.fs" />
    <Compile Include="FindSettings.fs" />
    <Compile Include="FindFile.fs" />
    <Compile Include="FindOptions.fs" />
    <Compile Include="Finder.fs" />
    <Compile Include="FindMain.fs" />
    <None Remove="Resources\filetypes.json" />
    <None Remove="Resources\filetypes.xml" />
    <None Remove="Resources\findoptions.json" />
    <None Remove="Resources\findoptions.xml" />
    <EmbeddedResource Include="Resources\filetypes.json" />
    <EmbeddedResource Include="Resources\filetypes.xml" />
    <EmbeddedResource Include="Resources\findoptions.json" />
    <EmbeddedResource Include="Resources\findoptions.xml" />
  </ItemGroup>

</Project>
