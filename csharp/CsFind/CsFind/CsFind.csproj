<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>netcoreapp3.1</TargetFramework>
        <Nullable>enable</Nullable>
        <PackageVersion>0.1.0</PackageVersion>
        <Authors>Cary Clark</Authors>
        <Description>C# implementation of xfind in dotnet 3.1</Description>
        <RootNamespace>CsFind</RootNamespace>
    </PropertyGroup>

    <ItemGroup>
      <None Remove="Resources\findoptions.json" />
      <None Remove="Resources\filetypes.json" />
      <None Remove="Resources\findoptions.xml" />
      <EmbeddedResource Include="Resources\findoptions.xml" />
      <None Remove="Resources\filetypes.xml" />
      <EmbeddedResource Include="Resources\filetypes.xml" />
    </ItemGroup>

    <ItemGroup>
      <EmbeddedResource Include="Resources\findoptions.json" />
    </ItemGroup>

    <ItemGroup>
      <EmbeddedResource Include="Resources\filetypes.json" />
    </ItemGroup>

</Project>
